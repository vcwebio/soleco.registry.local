#!/usr/bin/env bash

export SOLECO_MODULE="registry.g1"

DOLLAR='$'
export MODECO_HOSTNAME="${DOLLAR}{CONTECO_HOSTNAME}"
export MODECO_MODULEPREFIX="${DOLLAR}{SOLECO_MODULEPREFIX}"
export MODECO_URLPREFIX="${DOLLAR}{SOLECO_URLPREFIX}"

export MODECO_CONFIGURATION_KEYS='\
$MODECO_HOSTNAME,\
$MODECO_MODULEPREFIX,\
$MODECO_URLPREFIX,'
export MODECO_CONFIGURATION_FILES="\
metrics_monitor_heartbeat_monitors_volume/registry.yml"

export MODECO_REGISTRY_REGISTRY_ENDPOINT_PORT="5001"
