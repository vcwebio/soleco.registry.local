#!/usr/bin/env bash

DOLLAR='$'
export MODECO_HOSTNAME="${DOLLAR}{CONTECO_HOSTNAME}"
export MODECO_MODULEPREFIX="${CONTECO_TYPE}_${CONTECO_NAME}"
export MODECO_URLPREFIX="${CONTECO_TYPE}"

export MODECO_CONFIGURATION_KEYS='\
$MODECO_HOSTNAME,\
$MODECO_MODULEPREFIX,\
$MODECO_URLPREFIX,'
export MODECO_CONFIGURATION_FILES="\
metrics_monitor_heartbeat_monitors_volume/registry.yml"

export MODECO_REGISTRY_REGISTRY_ENDPOINT_PORT="5000"
